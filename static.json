{
  "root": "public/",
  "headers": {
    "/**": {
      "Cache-Control": "public, max-age=0, must-revalidate"
    },
    "/**.css": {
      "Cache-Control": "public, max-age=31536000, immutable"
    },
    "/**.js": {
      "Cache-Control": "public, max-age=31536000, immutable"
    },
    "/static/**": {
      "Cache-Control": "public, max-age=31536000, immutable"
    },
    "/icons/*.png": {
      "Cache-Control": "public, max-age=31536000, immutable"
    }
  },
  "routes": {
    "/emailverify/**": "emailverify/index.html",
    "/sellerprofile/**": "sellerprofile/index.html",
    "/admin/reviewdetails/**": "reviewdetails/index.html",
    "/admin/resetpassword/**": "resetpassword/index.html",
    "/resetpassword/**": "resetpassword/index.html",
    "/userprofile/**": "userprofile/index.html"
  },
  "https_only": true,
  "error_page": "404.html"
}
